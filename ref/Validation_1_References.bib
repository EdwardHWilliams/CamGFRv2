journal = {Journal of Clinical Oncology : official journal of the American Society of Clinical Oncology},
month = {aug},
number = {24},
pages = {2798--2805},
pmid = {28686534},
title = {{New model for estimating glomerular filtration rate in patients with cancer.}},
url = {http://ascopubs.org/doi/10.1200/JCO.2017.72.7578 http://www.ncbi.nlm.nih.gov/pubmed/28686534 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5562175},
